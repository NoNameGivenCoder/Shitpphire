<header>
    <title>Sapphire Admin Panel</title>
</header>

<style>

    body {
        font-family: Arial, Helvetica, sans-serif;
    }

    tr, td, th {
        border: 2px solid black;
    }
</style>

<body>
    <img src="http://mii-images.account.nintendo.net/<%= account.hash %>_normal_face.png">

    <table style="border: black 2px solid; width: 1280px;">
        <tr><th>ID</th><th>Create Time</th><th>Admin</th><th>Action</th></tr>
        <% for (let i = 0; i < actions.length; i++) {
            const action = actions[i];%>

            <tr>
                <td><%= action.id %></td>
                <td><%= action.create_time %></td>
                <td><%= action.admin %></td>
                <td><%= action.action %></td>
            </tr>
        <%} %>
    </table>
</body>