<%- include('../partials/header', {
    account_needed : true,
    account : account[0]
}) %>


<body onload="wiiuBrowser.lockUserOperation(false);">
    <div class="nuBox">

        <h1 style="margin: 0;"><%= language.newUser.nnidCompleteHeader %></h1>

        <img style="border: 1px rgb(223, 223, 223) solid; padding: 5px; border-radius: 10px; background-color: white; margin: 20px;" src="http://mii-images.account.nintendo.net/<%= account[0].hash %>_happy_face.png" width="150px">

        <br>

        <span><%= language.newUser.nnidComplete %></span>
    </div>

    <a onclick="wiiuSound.playSoundByName('SE_WAVE_MENU', 1);" class="nuNext" href="/setup/04">Next</a>
</body>